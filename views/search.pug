extends layouts/main.pug
include mixins/videoBlock

block content
    .serach__header
        h3 Searching for : #{searchingBy}
    .search__videos
        if videos.length=== 0
            h5 No Videos Found
        each video in videos
            +videoBlock({
                videoFile : video.fileUrl,
                title : video.title,
                views : video.views,
                id : video.id
            })